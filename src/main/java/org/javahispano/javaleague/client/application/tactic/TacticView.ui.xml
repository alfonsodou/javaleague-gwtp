<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt">

	<ui:with field="nameTokens"
		type="org.javahispano.javaleague.client.place.NameTokens" />

	<ui:style>
	.buttons button {
		margin: 0.5em;
	}
	
	.labels span {
		margin-right: 5px;
	}
	
	.danger {
		background-color: #fcf2f2;
		border-color: #dFb5b4;
	}
	
	.margin-fix {
		margin-top: 0 !important;
	}
	</ui:style>

	<b:Row>
		<b:Column size="XS_12">
			<b.gwt:FormPanel ui:field="formPanelTactic" action="/upload">
				<!-- <g:FlowPanel addStyleNames="col-lg-10">
					<g:FileUpload name="fileUpload" />
				</g:FlowPanel> -->
				<b:Input type="URL">Fichero</b:Input>
				<g:FlowPanel addStyleNames="col-lg-10">
					<g:Button ui:field="updateTacticButton"></g:Button>
				</g:FlowPanel>
			</b.gwt:FormPanel>
		</b:Column>
	</b:Row>

</ui:UiBinder>