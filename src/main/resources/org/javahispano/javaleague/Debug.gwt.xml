<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "https://raw.githubusercontent.com/gwtproject/gwt/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='javaleague'>
    <inherits name="org.javahispano.javaleague.Common"/>

    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.google.gwt.user.Debug"/>
    <set-property name="gwt.logging.logLevel" value="INFO"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>

    <!-- Shared modules -->
    <extend-configuration-property name="gin.ginjector.modules"
                                   value="org.javahispano.javaleague.client.gin.DesktopModule"/>
    <extend-configuration-property name="gin.ginjector.modules"
                                   value="org.javahispano.javaleague.client.gin.CarStoreModule"/>
</module>
